<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jds.dsd.geograpp.herramientas.f_CalidadDelSuelo_sinArea"
    android:background="#ffffffff">

    <!-- TODO: Update blank fragment layout -->



    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tableLayout2"
        android:layout_below="@+id/radiogroup"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="7dp">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="bottom">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="RQD:"
                android:id="@+id/textView2"
                android:paddingLeft="10dp"
                android:textColor="#ff000000" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/editText2_RQD"
                android:textSize="10dp"
                android:textColor="#ff000000"
                android:elegantTextHeight="false"
                android:textColorLink="@color/accent_color"
                android:theme="@style/ThemeOverlay.AppCompat.Light" />

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Jr:"
                android:id="@+id/textView4"
                android:paddingLeft="10dp"
                android:textColor="#ff000000" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="20"
                android:id="@+id/editText4_Jr"
                android:textSize="10dp"
                android:textColor="#ff000000"
                android:elegantTextHeight="false"
                android:textColorLink="@color/accent_color"
                android:theme="@style/ThemeOverlay.AppCompat.Light"/>
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Jw:"
                android:id="@+id/textView5"
                android:paddingLeft="10dp"
                android:textColor="#ff000000" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="20"
                android:id="@+id/editText5_Jw"
                android:textSize="10dp"
                android:textColor="#ff000000"
                android:elegantTextHeight="false"
                android:textColorLink="@color/accent_color"
                android:theme="@style/ThemeOverlay.AppCompat.Light"/>
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Jn:"
                android:id="@+id/textView7"
                android:paddingLeft="10dp"
                android:textColor="#ff000000" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="20"
                android:id="@+id/editText6_Jn"
                android:textSize="10dp"
                android:textColor="#ff000000"
                android:elegantTextHeight="false"
                android:textColorLink="@color/accent_color"
                android:theme="@style/ThemeOverlay.AppCompat.Light"/>
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Ja:"
                android:id="@+id/textView8"
                android:paddingLeft="10dp"
                android:textColor="#ff000000" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="20"
                android:id="@+id/editText7_ja"
                android:textSize="10dp"
                android:textColor="#ff000000"
                android:elegantTextHeight="false"
                android:textColorLink="@color/accent_color"
                android:theme="@style/ThemeOverlay.AppCompat.Light"/>
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/tablaDatosCalidad">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="SRF:"
                android:id="@+id/textView9"
                android:paddingLeft="10dp"
                android:textColor="#ff000000" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="20"
                android:id="@+id/editText8_SRF"
                android:textSize="10dp"
                android:textColor="#ff000000"
                android:elegantTextHeight="false"
                android:textColorLink="@color/accent_color"
                android:theme="@style/ThemeOverlay.AppCompat.Light"/>
        </TableRow>

    </TableLayout>

    <Button
        android:layout_width="60dp"
        android:layout_height="60dp"

        android:id="@+id/button_calidadSuelo"

        android:background="@drawable/icono_calcular"
        android:backgroundTintMode="add"
        android:layout_below="@+id/tableLayout2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView10_Q1resultado"
        android:hint="Q1:"
        android:textColor="#ff000000"
        android:textColorHint="@color/accent_color"
        android:paddingLeft="10dp"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/textView10_Q2resultado"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView10_Q2resultado"
        android:hint="Q2:"
        android:textColor="#ff000000"
        android:textColorHint="@color/accent_color"
        android:paddingLeft="10dp"
        android:layout_marginTop="21dp"
        android:layout_below="@+id/button_calidadSuelo"
        android:layout_alignLeft="@+id/textView10_Q1resultado"
        android:layout_alignStart="@+id/textView10_Q1resultado" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:showDividers="middle"
        android:orientation="horizontal"
        android:layout_below="@+id/textViewToolBar_app2"
        android:layout_centerHorizontal="true"
        android:id="@+id/radiogroup"
        android:layout_marginTop="10dp">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Q1"
            android:id="@+id/radioButton"
            android:textColor="#ff000000"
            android:checked="true"
            android:enabled="true"
            android:theme="@style/ThemeOverlay.AppCompat.Light"/>

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Q2"
            android:id="@+id/radioButton2"
            android:layout_gravity="right"
            android:textColor="#ff000000"
            android:clickable="true"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:longClickable="false"
            android:theme="@style/ThemeOverlay.AppCompat.Light"/>
    </RadioGroup>

    <Button
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/buttonLimpiar"
        android:background="@drawable/limpiar"
        android:backgroundTintMode="add"
        android:layout_alignTop="@+id/radiogroup"
        android:layout_toRightOf="@+id/radiogroup"
        android:layout_toEndOf="@+id/radiogroup"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp" />

</RelativeLayout>

