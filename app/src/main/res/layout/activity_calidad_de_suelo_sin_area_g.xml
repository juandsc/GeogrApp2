<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jds.dsd.geograpp.herramientas.CalidadDeSuelo_SinAreaG"
    android:background="@android:color/white"
    android:popupTheme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">


    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.jds.dsd.geograpp.herramientas.f_CalidadDelSuelo_sinArea"
        android:id="@+id/fragment"
        android:layout_below="@+id/textViewToolBar_app2"
        tools:layout="@layout/fragment_f__calidad_del_suelo_sin_area"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Calidad de suelo"
        android:textColor="@android:color/white"
        android:id="@+id/textViewToolBar_app2"
        android:minHeight="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@color/primary_color"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        android:gravity="center" />


</RelativeLayout>
